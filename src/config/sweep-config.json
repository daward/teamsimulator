{
  "outputFile": "sweep-results.csv",
  "unitRandom": { "n": 300 },
  "unitMappings": {
    "u_env": { "target": "environment.newTaskRate", "min": 0.1, "max": 6, "type": "float" },
    "u_workers": { "target": "team.size", "min": 2, "max": 12, "type": "int" },
    "u_types": { "target": "team.numTaskTypes", "min": 2, "max": 30, "type": "int" },
    "u_value": { "target": "environment.avgTaskValue", "min": 1, "max": 20, "type": "float", "scale": "log" },
    "u_askProb": { "target": "behavior.askProbability", "min": 0.0, "max": 1.0, "type": "float" },
    "u_absence": { "target": "behavior.absenceProbability", "min": 0.0, "max": 0.2, "type": "float" },
    "u_decay": { "target": "behavior.forgetfulness", "min": 0.0, "max": 0.15, "type": "float" },
    "u_completionLearn": { "target": "behavior.completionLearningRate", "min": 0.0, "max": 0.2, "type": "float" },
    "u_convoLearn": { "target": "behavior.conversationLearningRate", "min": 0.0, "max": 1.0, "type": "float" },
    "u_askMinGain": { "target": "behavior.askMinimumGain", "min": 0.0, "max": 0.5, "type": "float" },
    "u_turnover": { "target": "turnover.probability", "min": 0.0, "max": 0.5, "type": "float" },
    "u_turnoverAvgFactor": { "target": "turnover.hireAvgFactor", "min": 0.4, "max": 1.0, "type": "float" },
    "u_turnoverMinBar": { "target": "turnover.minHireBar", "min": 0.1, "max": 0.9, "type": "float" },
    "u_turnoverSpecialistBoost": { "target": "turnover.specialistBoost", "min": 0.0, "max": 0.6, "type": "float" },
    "u_infoTime": { "target": "environment.avgInfoTime", "min": 5, "max": 100, "type": "float" },
    "u_implTime": { "target": "environment.avgImplTime", "min": 5, "max": 100, "type": "float" }
  }
}
