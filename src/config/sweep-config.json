{
  "outputFile": "sweep-results.csv",
  "unitRandom": { "n": 300 },
  "unitMappings": {
    "u_env": { "target": "envTaskRate", "min": 0.1, "max": 6, "type": "float" },
    "u_workers": { "target": "numWorkers", "min": 2, "max": 12, "type": "int" },
    "u_types": { "target": "numTaskTypes", "min": 2, "max": 30, "type": "int" },
    "u_value": { "target": "avgValue", "min": 1, "max": 20, "type": "float", "scale": "log" },
    "u_askProb": { "target": "askProb", "min": 0.0, "max": 1.0, "type": "float" },
    "u_absence": { "target": "absenceProb", "min": 0.0, "max": 0.2, "type": "float" },
    "u_decay": { "target": "knowledgeDecayRate", "min": 0.0, "max": 0.15, "type": "float" },
    "u_completionLearn": { "target": "completionLearningRate", "min": 0.0, "max": 0.2, "type": "float" },
    "u_convoLearn": { "target": "conversationLearningRate", "min": 0.0, "max": 1.0, "type": "float" },
    "u_askMinGain": { "target": "askMinGain", "min": 0.0, "max": 0.5, "type": "float" },
    "u_turnover": { "target": "turnoverProb", "min": 0.0, "max": 0.5, "type": "float" },
    "u_turnoverLag": { "target": "turnoverHireLag", "min": 0, "max": 20, "type": "int" },
    "u_turnoverAvgFactor": { "target": "turnoverHireAvgFactor", "min": 0.4, "max": 1.0, "type": "float" },
    "u_turnoverSpecialistBoost": { "target": "turnoverSpecialistBoost", "min": 0.0, "max": 0.6, "type": "float" },
    "u_infoImpl": { "target": "infoToImplRatio", "type": "ratio" }
  }
}
